# Docker Compose environment variables
# Copy this file to .env.docker and update the password before starting

# PostgreSQL
POSTGRES_USER=par_user
POSTGRES_PASSWORD=change-me-to-a-strong-password
POSTGRES_DB=par_db

# Database connection (must match the PostgreSQL values above)
DATABASE_URL=postgresql://par_user:change-me-to-a-strong-password@db:5432/par_db

# App port (optional, defaults to 3000)
# APP_PORT=3000
